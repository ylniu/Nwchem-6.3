echo
start o2_zora_so

geometry noautoz units angstrom
symmetry c1
 O 0 0 -0.60000000
 O 0 0  0.60000000
end

basis spherical
 * library "DZVP (DFT Orbital)"
end

dft
 grid xfine
 iterations 100
 XC b3lyp
 odft
 mult 3
end

driver
 maxiter 100
end

relativistic
  zora on
  zora:cutoff 1d-30
end

task sodft optimize
task sodft freq
