 argument  1 = dft_he2+.nw



============================== echo of input deck ==============================
echo

title "he2+ hcth functional"

memory 128 mb
#memory global 73 mb stack 20 mb heap 5 mb

start he2+

geometry units angstrom print
he 0.   0.58 0.
symmetry c2v
end

basis "ao basis"
he library "DZVP (DFT Orbital)"
he p
 0.183 1.
end

basis "cd basis"
he library DGauss_A2_DFT_Coulomb_Fitting
end

charge +1.0

dft
mult 2
#grid lebedev 100 8
grid medium
convergence energy 1d-10
XC hcth
end

task dft gradient

title "he2+ Becke 1997 functional"
dft
XC becke97-1
end

task dft gradient

title "XC perdew91 "
dft
XC xperdew91 perdew91
end

task dft gradient


title "XC pbe96 "
dft
XC xpbe96 cpbe96
end

task dft gradient

title "XC b3lyp "
dft
XC b3lyp
end

task dft gradient

title "XC ft97 "
dft
XC ft97
end

task dft gradient

title "XC BOP "
dft
XC bop
end

task dft gradient

title "XC TPSS03 "
dft
XC xtpss03 ctpss03
end

task dft gradient

title "XC PKZB "
dft
XC xpkzb99 cpkzb99
end

task dft gradient
